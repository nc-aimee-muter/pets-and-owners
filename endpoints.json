{
  "GET /api": {
    "description": "serves up a json representation of all the available endpoints of the api"
  },
  "GET /api/owners/:id": {
    "description": "servers an object of the requested owner",
    "idFormatExamples": "o1, o12",
    "exampleResponse": {
      "owner": {
        "id": "o1",
        "name": "Steve",
        "age": 28
      }
    }
  },
  "GET /api/owners": {
    "description": "serves an array of all owners",
    "exampleResponse": {
      "owners": [
        {
          "id": "o1",
          "name": "Steve",
          "age": 28
        },
        {
          "id": "o2",
          "name": "Lucy",
          "age": 19
        }
      ]
    }
  },
  "GET /api/owners/:id/pets": {
    "description": "serves an array of pets belonging to the requested owner",
    "idFormatExamples": "o1, o12",
    "exampleResponse": {
      "pets": [
        {
          "id": "p8",
          "name": "Mike Verternum",
          "avatarUrl": "http://lh5.ggpht.com/-t36nK4mKyeE/UpjQdOIcqtI/AAAAAAAAHUM/Q6VFu-Uvo9o/s1600/instaG_2013_11_29_15.01.34.jpg",
          "favouriteFood": "Bamboo",
          "owner": "o5",
          "age": 6,
          "temperament": "grumpy"
        }
      ]
    }
  }
}
